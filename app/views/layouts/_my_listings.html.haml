.panel.panel-default
  %table.table.table-striped
    %thead
      %th Name
      %th Rates
      %th Created
    %tbody
      - @listings.each do |listing|
        %tr
          %td.col-md-4
            = listing.name
          %td.col-md-5
            - listing.rates.each do |rate|
              %div
                = number_to_currency(rate.amount)
                = "/"
                = rate.date_range
          %td.col-md-3
            = listing.created_at.strftime("%B %d, %Y")